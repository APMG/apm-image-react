{
  "name": "@apmg/mimas",
  "description": "A React component that takes an image endpoint from APM's APIs and returns a proper image with srcset.",
  "version": "1.1.3",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "image",
    "react",
    "apm",
    "media"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/APMG/apm-mimas"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest --watch",
    "test:ci": "jest",
    "test:coverage": "jest --coverage",
    "eslint": "eslint .",
    "prettier": "prettier --check '**/**.js'",
    "prettier:fix": "prettier --check '**/**.js' --write",
    "clean": "rimraf node_modules dist package-lock.json yarn.lock"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn eslint && yarn prettier",
      "pre-push": "yarn eslint && yarn prettier && yarn test:ci"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "peerDependencies": {
    "next": "^8.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@testing-library/jest-dom": "^5.4.0",
    "@testing-library/react": "^10.0.2",
    "babel-eslint": "^10.0.3",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.6",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "husky": "^1.3.1",
    "jest": "^24.9.0",
    "jest-prop-type-error": "^1.1.0",
    "prettier": "1.16.4",
    "react": "^16.12.0",
    "react-dom": "^16.13.1",
    "react-router": "^5.1.2",
    "rimraf": "^3.0.2",
    "rollup": "^1.31.0",
    "rollup-plugin-babel": "^4.3.3"
  },
  "dependencies": {
    "loading-attribute-polyfill": "^1.5.4"
  }
}
