{
  "name": "apm-mimas",
  "description": "A React component that takes an image endpoint from APM's APIs and returns a proper image with srcset.",
  "version": "0.1.1",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "image",
    "react",
    "apm",
    "media"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/APMG/apm-mimas"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__",
    "test": "react-scripts test",
    "jest": "jest",
    "eject": "react-scripts eject",
    "eslint": "eslint ./src/**/**.js",
    "eslint:fix": "eslint ./src/**/**.js --fix",
    "prettier": "prettier --check './src/**/**.js'",
    "prettier:fix": "prettier --check './src/**/**.js' --write",
    "hook:setup": "sh setup-hooks.sh"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-react": "^7.12.4",
    "jest-prop-type-error": "^1.1.0",
    "prettier": "1.16.4",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-scripts": "2.1.5",
    "react-testing-library": "^5.8.0"
  }
}
