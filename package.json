{
  "name": "apm-mimas",
  "description": "A React component that takes an image endpoint from APM's APIs and returns a proper image with srcset.",
  "version": "0.1.6",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "image",
    "react",
    "apm",
    "media"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/APMG/apm-mimas"
  },
  "scripts": {
    "build": "webpack --mode production",
    "start": "webpack --mode development",
    "test": "jest --watch",
    "test:ci": "jest",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "eslint": "eslint .",
    "prettier": "prettier --check '**/**.js'",
    "prettier:fix": "prettier --check '**/**.js' --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run eslint && npm run prettier",
      "pre-push": "npm run eslint && npm run prettier"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "classnames": "^2.2.6",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.7.1",
    "jest-prop-type-error": "^1.1.0",
    "prettier": "1.16.4",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-testing-library": "^5.8.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "husky": "^1.3.1"
  }
}
